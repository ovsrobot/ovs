AT_BANNER([ovs-lldp])

AT_SETUP([lldp - check lldp neighbor display])

OVS_VSWITCHD_START([])

add_of_ports br0 1
AT_CHECK([
    ovs-vsctl set Interface p1 type=dummy lldp:enable=true
], [0])

AT_CHECK([ovs-appctl netdev-dummy/receive p1 \
''])

